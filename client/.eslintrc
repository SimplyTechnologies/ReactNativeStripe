{
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "react-native",
      "jsx-a11y",
      "import"
    ],
    "env": {
        "node": true, 
        "es6": true 
    },
    "extends": "airbnb",
    "rules": {
        "quotes": ["error", "double"],
        "comma-dangle": ["error", "never"]
    }
}